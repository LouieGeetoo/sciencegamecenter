<!DOCTYPE html>
<html>
  <%= render 'layouts/head' %>
  <body>
    <%= render 'layouts/top_banners' %>
    <%= render 'layouts/header' %>
    <div class="container" id="content-container">
      <%= render 'layouts/ie6_warning' %>
      <%= render 'layouts/flash', flash: flash %>

      <div class="row-fluid">
        <div class="span10">
          <%= yield %>
        </div>
        <%= render 'layouts/side_banners' %>
      </div>

      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>