<!DOCTYPE html>
<html>
  <%= render 'layouts/head' %>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
    	<% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>">
          <%= value %>
          <a class='close' data-dismiss='alert' href='#'>&times;</a>
        </div>
      <% end %>
      <%= yield %>
      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>